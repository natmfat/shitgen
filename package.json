{
  "name": "shitpost",
  "version": "1.0.0",
  "description": "A shit PostgreSQL type generator",
  "main": "index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./database/*": {
      "types": "./dist/database/*.d.ts",
      "default": "./dist/database/*.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc -p .",
    "start": "tsx ./src/index.ts",
    "start:model": "tsx ./src/database/Model.ts",
    "prebuild": "rimraf dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assert": "^2.1.0",
    "commander": "^12.1.0",
    "postgres": "^3.4.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "rimraf": "^6.0.1",
    "vitest": "^2.1.3"
  }
}
