{
  "name": "shitpost",
  "version": "0.0.0",
  "description": "A shit PostgreSQL type generator",
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "default": "./dist/client/*.js"
    },
    "./MockDatabase/*": {
      "types": "./dist/MockDatabase/*.d.ts",
      "default": "./dist/MockDatabase/*.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc -p .",
    "start": "tsx ./src/index.ts",
    "start:model": "tsx ./src/database/Model.ts",
    "prebuild": "rimraf dist/",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assert": "^2.1.0",
    "commander": "^12.1.0",
    "postgres": "^3.4.4",
    "promptly": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "@types/promptly": "^3.0.5",
    "rimraf": "^6.0.1",
    "vitest": "^2.1.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
